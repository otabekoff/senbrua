#!/usr/bin/make -f

export DEB_BUILD_MAINT_OPTIONS = hardening=+all

%:
	dh $@

override_dh_auto_configure:
	dh_auto_configure -- -Dprefix=/usr

override_dh_auto_build:
	npm ci
	npm run build
	dh_auto_build

override_dh_auto_install:
	dh_auto_install -- --destdir=$(CURDIR)/debian/uz.mohirlab.senbrua
